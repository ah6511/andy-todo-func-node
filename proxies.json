{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "proxyHomePage": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/"
      },
      "debug":true,
      "backendUri": "https://andytodofuncnode.z19.web.core.windows.net/index.html"
    },
    "proxyContent": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/content/{*restOfPath}"
      },
      "debug":true,
      "backendUri": "https://andytodofuncnode.z19.web.core.windows.net/content/{restOfPath}"
    }
  }
}